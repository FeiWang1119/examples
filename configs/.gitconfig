[https]
	proxy = https://127.0.0.1:7897
[http]
	proxy = http://127.0.0.1:7897
[core]
	editor = nvim
[user]
	name = wangfei
	email = wangfei@deepin.org
[help]
	autocorrect = 1
[pull]
	rebase = true
[remote]
	pushdefault = origin
[maintenance]
	repo = /home/fei/repo/job-data
	strategy = incremental
	repo = /home/fei/repo/qtbase
[rerere]
	enabled = true
	autoUpdate = true
[diff]
    tool = kitty
    guitool = kitty.gui
[difftool]
    prompt = false
    trustExitCode = true
[difftool "kitty"]
    cmd = kitten diff $LOCAL $REMOTE
[difftool "kitty.gui"]
    cmd = kitten diff $LOCAL $REMOTE
